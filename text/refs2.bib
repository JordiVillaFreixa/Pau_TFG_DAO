@article{motono_mdcontactcom_2021,
	title = {{MDContactCom}: a tool to identify differences of protein molecular dynamics from two {MD} simulation trajectories in terms of interresidue contacts},
	volume = {38},
	issn = {1367-4803},
	shorttitle = {{MDContactCom}},
	url = {https://doi.org/10.1093/bioinformatics/btab538},
	doi = {10.1093/bioinformatics/btab538},
	abstract = {Comparing results from multiple MD simulations performed under different conditions is essential during the initial stages of analysis. We propose a tool called MD Contact Comparison (MDContactCom) that compares residue-residue contact fluctuations of two MD trajectories, quantifies the differences, identifies sites that exhibit large differences and visualizes those sites on the protein structure. Using this method, it is possible to identify sites affected by varying simulation conditions and reveal the path of propagation of the effect even when differences between the 3D structure of the molecule and the fluctuation RMSF of each residue is unclear. MDContactCom can monitor differences in complex protein dynamics between two MD trajectories and identify candidate sites to be analyzed in more detail. As such, MDContactCom is a versatile software package for analyzing most MD simulations.MDContactCom is freely available for download on GitLab. The software is implemented in Python3. https://gitlab.com/chiemotono/mdcontactcom.Supplementary data are available at Bioinformatics online.},
	number = {1},
	urldate = {2023-10-19},
	journal = {Bioinformatics},
	author = {Motono, Chie and Yanagida, Shunsuke and Sato, Miwa and Hirokawa, Takatsugu},
	month = dec,
	year = {2021},
	pages = {273--274},
	file = {motono_et_al_2021_bioinformatics.pdf:/home/jordivilla/Zotero/storage/7LUHK4TL/motono_et_al_2021_bioinformatics.pdf:application/pdf;motono_et_al_2021_mdcontactcom.pdf:/home/jordivilla/Zotero/storage/H6LN8GHP/motono_et_al_2021_mdcontactcom.pdf:application/pdf;Snapshot:files/3016/6325079.html:text/html},
}

@article{chong_protein-protein_2021,
	title = {Protein-{Protein} {Interactions}: {Insight} from {Molecular} {Dynamics} {Simulations} and {Nanoparticle} {Tracking} {Analysis}},
	volume = {26},
	issn = {1420-3049},
	shorttitle = {Protein-{Protein} {Interactions}},
	url = {https://www.ncbi.nlm.nih.gov/pmc/articles/PMC8472368/},
	doi = {10.3390/molecules26185696},
	abstract = {Protein-protein interaction plays an essential role in almost all cellular processes and biological functions. Coupling molecular dynamics (MD) simulations and nanoparticle tracking analysis (NTA) assay offered a simple, rapid, and direct approach in monitoring the protein-protein binding process and predicting the binding affinity. Our case study of designed ankyrin repeats proteins (DARPins)—AnkGAG1D4 and the single point mutated AnkGAG1D4-Y56A for HIV-1 capsid protein (CA) were investigated. As reported, AnkGAG1D4 bound with CA for inhibitory activity; however, it lost its inhibitory strength when tyrosine at residue 56 AnkGAG1D4, the most key residue was replaced by alanine (AnkGAG1D4-Y56A). Through NTA, the binding of DARPins and CA was measured by monitoring the increment of the hydrodynamic radius of the AnkGAG1D4-gold conjugated nanoparticles (AnkGAG1D4-GNP) and AnkGAG1D4-Y56A-GNP upon interaction with CA in buffer solution. The size of the AnkGAG1D4-GNP increased when it interacted with CA but not AnkGAG1D4-Y56A-GNP. In addition, a much higher binding free energy (∆GB) of AnkGAG1D4-Y56A (−31 kcal/mol) obtained from MD further suggested affinity for CA completely reduced compared to AnkGAG1D4 (−60 kcal/mol). The possible mechanism of the protein-protein binding was explored in detail by decomposing the binding free energy for crucial residues identification and hydrogen bond analysis.},
	number = {18},
	urldate = {2023-10-19},
	journal = {Molecules},
	author = {Chong, Wei Lim and Chupradit, Koollawat and Chin, Sek Peng and Khoo, Mai Mai and Khor, Sook Mei and Tayapiwatana, Chatchai and Nimmanpipug, Piyarat and Thongkum, Weeraya and Lee, Vannajan Sanghiran},
	month = sep,
	year = {2021},
	pmid = {34577167},
	pmcid = {PMC8472368},
	pages = {5696},
	file = {chong_et_al_2021_molecules.pdf:/home/jordivilla/Zotero/storage/CS4GIZ8J/chong_et_al_2021_molecules.pdf:application/pdf;chong_et_al_2021_protein-protein_interactions.pdf:/home/jordivilla/Zotero/storage/K5CW8624/chong_et_al_2021_protein-protein_interactions.pdf:application/pdf;chong_et_al_2021_protein-protein_interactions.pdf:files/16862/chong_et_al_2021_protein-protein_interactions2.pdf:application/pdf;chong_et_al_2021_protein-protein_interactions.pdf:files/34335/chong_et_al_2021_protein-protein_interactions2.pdf:application/pdf;chong_et_al_2021_protein-protein_interactions.pdf:files/34336/chong_et_al_2021_protein-protein_interactions3.pdf:application/pdf;chong_et_al_2021_protein-protein_interactions.pdf:files/34337/chong_et_al_2021_protein-protein_interactions4.pdf:application/pdf;chong_et_al_2021_protein-protein_interactions.pdf:files/34338/chong_et_al_2021_protein-protein_interactions5.pdf:application/pdf},
}

