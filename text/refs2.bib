@article{islam_n-acetyl-d-glucosamine-asparagine_1991,
	title = {N-{Acetyl}-{D}-glucosamine-{Asparagine} {Structure} in {Ribosomeinactivating} {Proteins} from the {Seeds} of {Luffa} cylindrica and {Phytolacca} americana},
	volume = {55},
	doi = {10.1271/bbb1961.55.1375},
	abstract = {Glycosylation-site-containing peptides were isolated from the proteolytic digests of lutfin-a, luffin-b, PAP-S and CNBr-fragments of PAP-S by reverse-phase HPLC, and their amino acid compositions and sequences were analyzed. Six peptides were obtained from luffin-a, and three each from luffin-b and PAP-S. All of these peptides were negative toward the phenol-H2SO4 reaction and gave only N-acetyl-D-glucosamine in gas chromatography after methanolysis and reacetylation. Amounts of N-acetyl-D-glucosamine in these peptides were determined as D-glucosamine to be approximately one mol per peptide by an amino acid analyzer after HC1 hydrolysis. Based on these results we concluded that Asn residues at positions of 28, 33, 77, 84, 206, and 227 in luffin-a, of 2, 78, and 85 in luffin-b, and of 10, 44, and 255 in PAP-S were glycosylated with only GlcNAc, and contained one residue per site.},
